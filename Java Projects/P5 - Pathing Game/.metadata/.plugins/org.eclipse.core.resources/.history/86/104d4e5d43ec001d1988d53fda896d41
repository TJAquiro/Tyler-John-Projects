
public class Passage extends Block {
	
	private int value;
	
	/**
	    * Constructor for Obstacle Object
	    * validates inputs
	    *
	    * @param label the Obstacle's Name
	    * @return none
	    * */
	
	public Passage(int value)
	{
		this.value = value;
	}
	
	public String toString()
	{
		return (value + "");
	}
	
	public int getValue()
	{
		return value;
	}
}
