
public class Obstacle extends Block {

	private String label;
	
	/**
	    * Constructor for Obstacle Object
	    * validates inputs
	    *
	    * @param label the Obstacle's Name
	    * @return none
	    * */
	
	public Obstacle(String label)
	{
		this.label = label;
	}
	
	/**
	    * @override
	    * Overrides toString() method
	    *
	    * @param none
	    * @return String, "instructor: instructor, credit: credit, meetDays: meetDays, gtas: gtas,type: type, CRN: crn, title: title, levels: levels"
	    * */
	public String toString()
	{
		return(label);
	}
	
	@Override
	public int getValue() {
		
		return 0;
	}

}
